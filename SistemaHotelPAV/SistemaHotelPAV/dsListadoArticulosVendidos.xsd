                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Articulos.id_art, Articulos.nombre, Tipos.nombre AS Tipo, SUM(DetallesFactura.cantidad) AS Cantidad
FROM            Usuarios INNER JOIN
                         Facturas ON Usuarios.id_usu = Facturas.id_usu INNER JOIN
